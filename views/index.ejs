<% obj.forEach(function(player){ %>
  <style>
  table {
    font-family: arial, sans-serif;
    border-collapse: collapse;
  }

  td, th {
    border: 1px solid #dddddd;
    text-align: left;
    padding: 8px;
  }

  tr:nth-child(even) {
    background-color: #dddddd;
  }
  </style>
  <table>
<tr>
  <td><%= player.name %></td>
  <td><%= player.Matches %></td>
  <td><%= player.Runs %></td>
</tr>
  </table>
<% })%>
